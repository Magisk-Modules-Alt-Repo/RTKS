Return() {
    printf "\033c"
    unlockermenu
    printf "\033c"
}

Back() {
    printf "\033c"
    rtksmenu
    printf "\033c"
}

GoOut() {
    printf "\033c"
    echo ""
    echo "${B}Bye bye, see you next time.${N}"
    exit
}
printf "\033c"
Menu
